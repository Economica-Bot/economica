{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"composite": true,
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"jsx": "preserve",
		"noEmit": true,
		"incremental": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		"**/*.mjs",
		"**/*.cjs",
		".next/types/**/*.ts"
	]
}
