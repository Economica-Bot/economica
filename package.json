		"eslint-import-resolver-typescript": "^2.7.0",
		"eslint-plugin-import": "^2.25.4",
		"husky": "^8.0.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.5.5",
		"typescript-eslint": "^0.0.1-alpha.0"
	}
