{
	"name": "economica",
	"version": "1.0.0",
	"main": "dist/index.js",
	"type": "module",
	"bugs": {
		"url": "https://github.com/economica-bot/economica/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/economica-bot/economica.git"
	},
	"contributors": [
		"Adrastopoulos#6969",
		"QiNG-agar#0540"
	],
	"engines": {
		"node": ">=16.9.0",
		"yarn": ">=1.22.0",
		"npm": "please-use-yarn"
	},
	"scripts": {
		"lint": "eslint .",
		"format": "eslint --fix .",
		"prepare": "yarn husky install",
		"clean": "./node_modules/.bin/rimraf dist",
		"prebuild": "yarn run clean",
		"build": "esbuild ./src/index.ts --platform=node --format=esm --external:./node_modules/* --bundle --sourcemap --minify-whitespace --minify-syntax --outdir=dist",
		"start": "node --enable-source-maps --es-module-specifier-resolution=node --experimental-modules dist/index.js",
		"docker": "docker compose up --build --force-recreate"
	},
	"license": "ISC",
	"dependencies": {
		"@adrastopoulos/number-parser": "github:Adrastopoulos/Number-Parser",
		"@types/express-serve-static-core": "^4.17.30",
