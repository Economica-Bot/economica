		"cors": "^2.8.5",
		"discord-api-types": "^0.36.2",
		"discord.js": "^14.0.1",
		"dotenv": "^10.0.0",
		"esbuild": "^0.14.48",
		"express": "^4.17.3",
		"express-session": "^1.17.2",
		"fs": "^0.0.1-security",
		"json-format": "^1.0.1",
		"module-alias": "^2.2.2",
		"ms": "^2.1.3",
		"node-fetch": "^3.2.3",
		"passport": "^0.5.2",
		"passport-discord": "^0.1.4",
