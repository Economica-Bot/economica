{
	"name": "@economica/bot",
	"version": "1.0.0",
	"main": "dist/index.js",
	"type": "module",
	"engines": {
		"node": ">=16.9.0",
		"yarn": ">=1.22.0",
		"npm": "please-use-yarn"
	},
	"scripts": {
		"lint": "eslint .",
		"format": "eslint --fix .",
		"clean": "./node_modules/.bin/rimraf dist",
		"prebuild": "yarn run clean",
		"build": "esbuild ./src/index.ts --platform=node --format=esm --external:./node_modules/* --external:../../node_modules/* --bundle --minify --sourcemap --outdir=dist",
		"start": "node --enable-source-maps --es-module-specifier-resolution=node --experimental-modules dist/index.js",
		"dev": "yarn run build && yarn run start",
		"docker": "docker compose up --build --force-recreate"
	},
	"license": "ISC",
	"dependencies": {
		"@adrastopoulos/number-parser": "github:Adrastopoulos/Number-Parser",
		"@discordjs/builders": "^1.2.0",
		"@types/express-serve-static-core": "^4.17.30",
		"body-parser": "^1.19.2",
		"connect-pg-simple": "^7.0.0",
		"cors": "^2.8.5",
		"discord-api-types": "^0.36.2",
		"discord.js": "^14.3.1-dev.1661731841-682e0e1.0",
		"dotenv": "^10.0.0",
		"esbuild": "^0.15.7",
		"express": "^4.17.3",
		"express-session": "^1.17.2",
		"fs": "^0.0.1-security",
		"json-format": "^1.0.1",
		"module-alias": "^2.2.2",
		"ms": "^2.1.3",
		"node-fetch": "^3.2.10",
		"passport": "^0.6.0",
		"passport-discord": "^0.1.4",
		"passport-oauth2": "^1.6.1",
		"path-to-regexp": "^6.2.1",
		"pg": "^8.7.3",
		"qs": "^6.11.0",
		"quickchart-js": "^1.3.1",
		"reflect-metadata": "^0.1.13",
		"tslog": "^3.3.1",
		"typeorm": "^0.3.6"
	},
	"devDependencies": {
		"@types/connect-pg-simple": "^7.0.0",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/express-session": "^1.17.4",
		"@types/ms": "^0.7.31",
		"@types/node": "^17.0.21",
		"@types/passport": "^1.0.7",
		"@types/passport-discord": "^0.1.5",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.5.5",
		"typescript-eslint": "^0.0.1-alpha.0"
	}
}
