{
	"name": "@economica/bot",
	"version": "1.0.0",
	"main": "./src/index.ts",
	"type": "module",
	"engines": {
		"node": ">=16.9.0",
		"yarn": ">=1.22.0",
		"npm": "please-use-yarn"
	},
	"scripts": {
		"lint": "eslint .",
		"format": "prettier . --write --ignore-path ../../.gitignore",
		"clean": "./node_modules/.bin/rimraf dist",
		"prebuild": "yarn run clean",
		"build": "esbuild ./src/index.ts --platform=node --format=esm --sourcemap --minify --bundle --external:./node_modules/* --external:../../node_modules/* --outdir=dist",
		"start": "node --enable-source-maps --es-module-specifier-resolution=node --experimental-modules dist/index.js",
		"dev": "yarn run build && yarn run start",
		"docker": "docker compose --env-file ../../.env up --build --force-recreate"
	},
	"license": "ISC",
	"dependencies": {
		"@adrastopoulos/number-parser": "github:Adrastopoulos/Number-Parser",
		"@discordjs/builders": "^1.2.0",
		"@discordjs/collection": "^1.1.0",
		"@discordjs/rest": "^1.1.0",
		"@sapphire/snowflake": "^3.2.2",
		"@trpc/server": "^10.0.0-proxy-beta.21",
		"cors": "^2.8.5",
		"discord-api-types": "^0.37.5",
		"discord-interactions": "^3.2.0",
		"dotenv": "^10.0.0",
		"esbuild": "^0.15.7",
		"express": "^4.17.3",
		"module-alias": "^2.2.2",
		"ms": "^2.1.3",
		"node-fetch": "^3.2.10",
		"path-to-regexp": "^6.2.1",
		"pg": "^8.7.3",
		"quickchart-js": "^1.3.1",
		"reflect-metadata": "^0.1.13",
		"tslog": "^3.3.1",
		"tweetnacl": "^1.0.3",
		"typeorm": "^0.3.6",
		"zod": "^3.19.1"
	},
	"devDependencies": {
		"@types/connect-pg-simple": "^7.0.0",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/express-session": "^1.17.4",
		"@types/ms": "^0.7.31",
		"@types/node": "^17.0.21",
		"@types/passport": "^1.0.7",
		"@types/passport-discord": "^0.1.5",
		"eslint": "^8.23.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"ts-to-zod": "^1.13.1",
		"typescript": "^4.5.5"
	}
}
