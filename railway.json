{
	"$schema": "https://railway.app/railway.schema.json",
	"build": {
		"builder": "NIXPACKS",
		"nixpacksPlan": {
			"phases": {
				"setup": {
					"nixPkgs": [
						"...",
						"nodePackages.node-gyp-build","python39"
					]
				}
			}
		}
	},
	"deploy": {
		"numReplicas": 1,
		"restartPolicyType": "ON_FAILURE",
		"restartPolicyMaxRetries": 10
	}
}
